<?xml version="1.0" encoding="UTF-8"?>
<quiqqer>
    <settings>

        <config >
            <section name="dbadmin">
                <conf name="databases">
                    <type><![CDATA[string]]></type>
                 
                </conf>
            </section>
            
        </config>

        <window>
            <title>DBAdmin</title>
            <params>
                <icon>fa fa-database</icon>
            </params>

            <categories>
                <category name="dbadmin">
                    <text>DBAdmin</text>
                    <icon>fa fa-database</icon>

                    <settings title="dbadmin" name="dbadmin">
                        <title>DBAdmin</title>
                        <input conf="dbadmin.databases" type="text">
                            <text>
                                <locale group="quiqqer/dbadmin" var="settings.dbadmin.databases.title" />
                            </text>
                            <description>
                                <locale group="quiqqer/dbadmin" var="settings.dbadmin.databases.desc" />
                            </description>
                        </input>
                    </settings>

                   
                </category>
            </categories>
        </window>

    </settings>
</quiqqer>
